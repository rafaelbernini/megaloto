{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" },
    { "src": "api/**/*.mjs", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.mjs" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/api/megasena/(.*)",
            "dest": "/api/megasena.mjs"
        },
        {
            "src": "(.*)",
            "dest": "/index.html"
        }
    ]
}